
package algoritmodeeuclides;

public class AlgoritmoDeEuclides {

    
    public static void main(String[] args) {
        
       int x, y, q, a = 1;
       
       x = 92;
       y = 72;
     
       while (a == 1){
       
          q = x%y;
          
          x = y;
          y = q;
          
          if (q == 0){
          
              System.out.println("MDC: " + x);
              break;
          
          }       
       
       }    
        
    }
    
}
